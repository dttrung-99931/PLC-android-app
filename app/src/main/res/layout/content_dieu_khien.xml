<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:showIn="@layout/fragment_dieu_khien">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_10sdp">

        <TextView
            android:id="@+id/lnLayoutNsx"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_18sdp"
            android:layout_marginLeft="@dimen/_20sdp"
            android:text="- Giá trị analog gửi về:"
            android:textColor="#07197E"
            android:textSize="16sp"
            android:layout_weight="1"/>

        <TextView
            android:id="@+id/tvProuctionDate"
            android:layout_width="@dimen/_50sdp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="76dp"
            android:text="100"
            android:layout_gravity="center_vertical"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_5sdp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_18sdp"
            android:layout_marginLeft="@dimen/_20sdp"
            android:text="- Giá trị digital gửi về:"
            android:textColor="#07197E"
            android:textSize="16sp"
            android:layout_weight="1"/>

        <TextView
            android:id="@+id/tvExpiryDate"
            android:layout_width="@dimen/_50sdp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="76dp"
            android:text="100"
            android:layout_gravity="center_vertical"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_15sdp">

        <TextView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_18sdp"
            android:layout_marginLeft="20dp"
            android:text="- Giá trị tần số cấp 1:"
            android:textColor="#0E8013"
            android:textSize="16sp"
            android:layout_weight="1"/>

        <TextView
            android:id="@+id/tvTssc1"
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:text="100"
            android:layout_gravity="center_vertical"
            android:layout_marginEnd="91dp" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/_20sdp">

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_18sdp"
            android:text="- Nhập giá trị cài đặt cho tần số cấp 1:"
            android:textColor="#0E8013"
            android:textSize="16sp"
            android:layout_weight="1"
            android:layout_marginBottom="@dimen/_10sdp"/>

        <EditText
            android:id="@+id/edtTssc1"
            android:layout_width="@dimen/_40sdp"
            android:layout_height="wrap_content"
            android:inputType="number"
            android:layout_marginStart="@dimen/_20sdp" />

        <Button
            android:id="@+id/btnOkTssc1"
            android:layout_width="@dimen/_50sdp"
            android:layout_height="@dimen/_30sdp"
            android:layout_marginStart="@dimen/_10sdp"
            android:padding="0dp"
            android:text="OK" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_15sdp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_18sdp"
            android:layout_marginLeft="20dp"
            android:text="- Giá trị tần số cấp 2:"
            android:textColor="#B3700E"
            android:textSize="16sp"
            android:layout_weight="1"/>

        <TextView
            android:id="@+id/tvTsc2"
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:text="100"
            android:layout_gravity="center_vertical"
            android:layout_marginEnd="91dp" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/_20sdp">

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_18sdp"
            android:text="- Nhập giá trị cài đặt cho tần số cấp 2:"
            android:textColor="#B3700E"
            android:textSize="16sp"
            android:layout_weight="1"
            android:layout_marginBottom="@dimen/_10sdp"/>

        <EditText
            android:id="@+id/edtTsc2"
            android:layout_width="@dimen/_40sdp"
            android:layout_height="wrap_content"
            android:inputType="number"
            android:layout_marginStart="@dimen/_20sdp"/>

        <Button
            android:id="@+id/btnUpdateFailureProductLimit"
            android:layout_width="@dimen/_50sdp"
            android:layout_height="@dimen/_30sdp"
            android:text="OK"
            android:layout_marginStart="@dimen/_10sdp"
            android:padding="0dp"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_15sdp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_18sdp"
            android:layout_marginLeft="20dp"
            android:text="- Điều khiển động cơ chạy"
            android:textColor="#9C27B0"
            android:textSize="16sp"
            android:layout_weight="1"/>

        <CheckBox
            android:id="@+id/cbDcc"
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginEnd="91dp"
            android:clickable="false"/>

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/_20sdp">

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_18sdp"
            android:text="- Bât động cơ:"
            android:textColor="#9C27B0"
            android:textSize="16sp"
            android:layout_weight="1"
            android:layout_marginBottom="@dimen/_10sdp"/>

        <Switch
            android:id="@+id/swDcc"
            android:layout_width="@dimen/_40sdp"
            android:layout_height="wrap_content"
            android:inputType="number"
            android:layout_marginStart="@dimen/_20sdp"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_10sdp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_18sdp"
            android:layout_marginLeft="20dp"
            android:text="- Điều khiển động cơ tắt"
            android:textColor="#E91E63"
            android:textSize="16sp"
            android:layout_weight="1"/>

        <CheckBox
            android:id="@+id/cbTdc"
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginEnd="91dp"
            android:clickable="false"/>

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/_20sdp">

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_18sdp"
            android:text="- Tắt động cơ:"
            android:textColor="#E91E63"
            android:textSize="16sp"
            android:layout_weight="1"
            android:layout_marginBottom="@dimen/_10sdp"/>

        <Switch
            android:id="@+id/swTdc"
            android:layout_width="@dimen/_40sdp"
            android:layout_height="wrap_content"
            android:inputType="number"
            android:layout_marginStart="@dimen/_20sdp"/>

    </LinearLayout>
</LinearLayout>